{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "icons":{
        "128": "jomo.png"
    },
    "content_scripts": [
        {
          "matches": ["https://linkedin.com/*"],
          "css": ["content.css"],
          "js": ["jomo_script.js"]
        }
    ],
    "permissions": ["storage"],
    "action": {
      "default_popup": "./index.html",
      "default_icon": "jomo.png",
      "default_width": 500,
      "default_height": 400 
    }
  }